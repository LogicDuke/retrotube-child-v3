{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "tmw/home-accordion-frame",
  "title": "TMW Home Accordion Frame",
  "category": "widgets",
  "icon": "list-view",
  "description": "Accordion frame with editable Gutenberg content (Models/Categories/Videos).",
  "textdomain": "retrotube-child",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "openByDefault": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./editor.js",
  "render": "file:./index.php"
}
